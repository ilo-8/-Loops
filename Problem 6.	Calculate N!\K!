using System;
					
public class Program
{
	public static void Main()
	{
		Console.Write("Plyce Enter n and k nubers (1<k<n<100)");
		int n = int.Parse(Console.ReadLine());
		int k = int.Parse(Console.ReadLine());
		int faktorialn = 1;
		int faktorialk = 1;
		int sum = 0;
		for	(int i = 1 ; i <= n ; i++ )
		{ 
			faktorialn *=i;
			if (i <= k)
             {
                faktorialk *= i;
				continue;				
             }								
		}
		sum = faktorialn/faktorialk;
		Console.WriteLine("Sum={0}",sum);
	}
}
	
