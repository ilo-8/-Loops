using System;


public class Program
{
	public static void Main()
	{
		Console.WriteLine("Plеасe Enter n nuber (1<=n<=20)");
		int n = int.Parse(Console.ReadLine());			
		for	(int i = 0 ; i <n ; i++ )
		{ 			
			for	(int j = 1+i ; j <=n+i; j++ )
			{				
				Console.Write(j+"");
			}
			Console.WriteLine();
		}		
	}	
}
	
